package arrays;

public class Demo_1D {

	public static void main(String[] args) {
		
		int ar1[]=new int[6];
		ar1[0]= 2;
		ar1[1]=3;
		ar1[2]=6;
		ar1[3]=4;
		ar1[4]=1;
		ar1[5]=9;
		System.out.println("printing ar1");
		for (int y:ar1) {
			System.out.print(y);
		}
		int newelement=5;
		int pos=2;
		int n=6;
		System.out.println("inserting 5 at position 2 (i.e., index 1)");
		for(int i=n;i>=pos;i--) {
			ar1[n]=ar1[n-1];
					
		}
		ar1[1]= newelement;
		n++;
		for(int z:ar1) {
			System.out.println(z);
		}
	}

}
